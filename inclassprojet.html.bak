h1 {
    font-family: Arial, Helvetica, sans-serif;
    text-align: center;
  }
  
  #cube .front {
    transform: 
      translateZ(100px);
	border-radius: 10px;  
  }
  
  #cube .back {
    transform: 
      rotateX(-180deg)
      translateZ(100px);
	border-radius: 10px;  
  }
  
  #cube .right {
    transform:
      rotateY(90deg)
      translateZ(100px);
	border-radius: 10px;  
  }
  
  #cube .left {
    transform:
      rotateY(-90deg)
      translateZ(100px);
	border-radius: 10px;  
  }
  
  #cube .top {
    transform:
      rotateX(90deg)
      translateZ(100px);
	border-radius: 10px;  
  }
  
  #cube .bottom {
    transform:
      rotateX(-90deg)
      translateZ(100px);
	border-radius: 10px;  
  }
  
  .container {
    width: 200px;
    height: 300px;
    position: relative;
    margin: 0 auto 40px;
    
    
    perspective: 1000px;
    perspective-origin: 50% 100%;
  }
  
  #cube {
    width: 100%;
    height: 100%;
    top: 100px;
    position: absolute;
    
    transform-style: preserve-3d;
    
    transition: transform 3s;
  }
  
  #cube:hover {
    cursor: pointer;
  }
  
  #cube div {
    background: hsla(0, 85%, 50%, 0.8);
    display: block;
    position: absolute;
    width: 200px;
    height: 100px;
    border: 2px solid #ab1a1a;
    
